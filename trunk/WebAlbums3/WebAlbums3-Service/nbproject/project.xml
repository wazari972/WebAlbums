<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>WebAlbums3-Service</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <included-library>file.reference.CP-Apache-commos-lang.2.5.jar</included-library>
            <included-library>file.reference.CP-EXIF-metadata-extractor-2.3.1.jar</included-library>
            <included-library>file.reference.CP-DB-dbunit-2.4.5.jar-1</included-library>
            <included-library>reference.WebAlbums3-DAO.dist</included-library>
            <included-library>reference.WebAlbums3-Util.jar</included-library>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>WebAlbums3-DAO</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>dist</target>
                <clean-target>clean</clean-target>
                <id>dist</id>
            </reference>
            <reference>
                <foreign-project>WebAlbums3-Util</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
