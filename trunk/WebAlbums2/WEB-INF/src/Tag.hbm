<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	  "-//Hibernate/Hibernate Mapping DTD//EN"
	  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="entity">
  <class name="Tag" table="Tag">
    <cache usage="nonstrict-read-write"/>
    <id name="ID" type="integer">
      <column name="ID" sql-type="int"/>
      <generator class="increment"/>
    </id>
    
    <property name="Nom" type="string" update="true">
      <column name="Nom" sql-type="char(100)" not-null="true" />
    </property>
    
    <property name="TagType" type="integer" index="tagtype" update="false">
      <column name="TagType" sql-type="int" not-null="true" />
    </property>
  </class>
</hibernate-mapping>
