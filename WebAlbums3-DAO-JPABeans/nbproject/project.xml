<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>WebAlbums3-DAO-JPABeans</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <included-library dirs="200">reference.WebAlbums3-iDAO.jar</included-library>
            <included-library dirs="200">reference.WebAlbums3-Commons.jar</included-library>
            <included-library dirs="200">file.reference.CP-Perf-perf4j-0.9.13.jar</included-library>
            <included-library dirs="200">file.reference.CP-Apache-commons-lang.2.5.jar</included-library>
