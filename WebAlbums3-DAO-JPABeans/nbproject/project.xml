            <reference>
                <foreign-project>WebAlbums3-iDAO</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>nbbuild.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
